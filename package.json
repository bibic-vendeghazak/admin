{
  "name": "bibic-vendeghazak-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "babel-eslint": "^8.2.6",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "firebase": "^5.4.0",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "material-ui": "^0.20.2",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "node-sass": "^4.9.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "query-string": "5",
    "query-string": "6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "^0.8.3",
    "styled-components": "^3.4.2",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "build-css": "node-sass-chokidar  --include-path ./src/styles/sass src/styles/sass/main.sass src/styles/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar -w --include-path ./src/styles/sass src/styles/sass/main.sass src/styles/main.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1"
  }
}
