{
  "name": "bibic-vendeghazak-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.10.1",
    "babel-eslint": "^8.2.3",
    "firebase-admin": "^5.12.0",
    "firebase-functions": "^1.0.1",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "moment-range": "^4.0.0",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "query-string": "5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "build-css": "node-sass-chokidar  --include-path ./src/styles/sass src/styles/sass/main.sass src/styles/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar -w --include-path ./src/styles/sass src/styles/sass/main.sass src/styles/main.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0"
  }
}
